{
	"name": "trackmage/trackmage-wordpress-plugin",
	"type": "wordpress-plugin",
	"description": "Official WordPress plugin for TrackMage",
	"keywords": [
		"trackmage"
	],
	"homepage": "https://github.com/trackmage/trackmage-wordpress-plugin",
	"license": "GPL-2.0-or-later",
	"require": {
		"ext-json": "*",
		"brightnucleus/config": "^0.4",
		"psr/log": "^1.1",
		"symfony/property-access": "^3.4|^4.3",
		"trackmage/trackmage-sdk-php": "dev-shipments"
	},
	"require-dev": {
		"ext-intl": "*",
		"automattic/vipwpcs": "^2.0",
		"dealerdirect/phpcodesniffer-composer-installer": "^0.5.0",
		"phpcompatibility/phpcompatibility-wp": "*",
		"sllh/composer-versions-check": "^2.0",
		"squizlabs/php_codesniffer": "^3.4.2",
		"wp-coding-standards/wpcs": "^2.1.1",
		"lucatume/wp-browser": "^2.2"
    },
	"conflict": {
		"phpunit/phpunit": ">=7.5.3"
	},
	"config": {
		"sort-packages": true
	},
	"autoload": {
		"psr-4": {
			"TrackMage\\WordPress\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"TrackMage\\WordPress\\Tests\\": "tests/"
		}
	},
	"support": {
		"issues": "https://github.com/trackmage/trackmage-wordpress-plugin/issues",
		"source": "https://github.com/trackmage/trackmage-wordpress-plugin"
	}
}
